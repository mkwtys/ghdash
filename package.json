    "react": "16.0.0-alpha.13",
    "react-dom": "16.0.0-alpha.13"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
