    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "cpx": "1.5.0",
    "electron": "1.6.11",
    "eslint": "4.1.1",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-flowtype": "2.34.1",
