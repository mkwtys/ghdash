{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "standard",
  "globals": {
  },
  "rules": {
    "camelcase": "warn",
    "no-extra-semi": "error",
    "prefer-const": "error",
    "semi": ["error", "always"],
    "space-before-function-paren": ["error", "never"],
    "spaced-comment": "off"
  }
}
